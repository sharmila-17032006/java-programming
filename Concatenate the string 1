import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sham=new Scanner(System.in);
        String str1= sham.next();
        String str2= sham.next();
        String str3 = str1 + str2;
        System.out.println(str3);
        
    }
}
